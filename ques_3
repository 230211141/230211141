#include<stdio.h>
int main()
{
    int a[100],i,n,sume=0,sumo=0;
    printf("enter the number of element \n");
    scanf("%d",&n);
    printf("enter the array element \n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        
    }
    printf("the sum even and odd element of array as follow");
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            printf("%d is even ",a[i]);
            sume=sume+a[i];
        }
        else
        {
            printf("%d is odd \n",a[i]);
            sumo=sumo+a[i];
        }
    }
    printf("the sum of even element is %d \n",sume);
    printf("the sum of odd element is %d \n",sumo);
    return 0;
}