#include <stdio.h>

int main() {
    int m, n, num;
    printf("Enter the number of rows: ");
    scanf("%d", &m);
    printf("Enter the number of columns: ");
    scanf("%d", &n);
    printf("Enter the number you want to fill in the matrix: ");
    scanf("%d", &num);
    int matrix[m][n];
    for (int i = 0; i < m; i++)
    {
        for (int j = 0; j < n; j++) 
        {
            matrix[i][j] = num;
        }
    }
    printf("\nThe resulting matrix is:\n");
    for (int i = 0; i < m; i++) 
    {
        for (int j = 0; j < n; j++)
        {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }

    return 0;
}
